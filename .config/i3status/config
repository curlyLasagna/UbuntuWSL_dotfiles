general {
	colors = true
	color_good = "#BBBBBB"
	color_bad = "#CC1616"
	color_degraded = "#55858E"
	interval = 1
}

order += "cpu_temperature 0"
order += "memory"
order += "volume master"
order += "ethernet eno1"
order += "tztime local"

cpu_temperature 0 {
	format = "%degrees Â°C"
	path = "/sys/devices/platform/coretemp.0/hwmon/hwmon1/temp1_input"
}

ethernet eno1 {
	format_up = " We coo "
	format_down = " Cmon bruh "
}

tztime local {
	format = " %h-%d %l:%M %p "
}

memory {
	format = " %used "	
}

volume master {
	format = " Vol %volume "
	format_muted = " Mute " 
	device = "default"
	mixer = "Master"
	mixer_idx = 0
}
